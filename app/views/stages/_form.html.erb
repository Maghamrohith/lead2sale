<%=form_for @stage do |f| %>
	<div class="form-group">
		<label>Name</label><br/>
		<%= f.text_field :name,class: "form-control" %><br/>
	</div>
	<div class ="form-group">
		<label>Description</label><br/>
		<%= f.text_area :description, class: "form-control"%><br/>
		
	</div>
	<div class="form-group">
	<label>InterestLevel</label><br/>
	<%= f.collection_radio_buttons :interest_level, (1..5).map { |n| [n, n] }, :first, :last,class: "form-control" %><br/>
		
	</div>
	<%= f.submit %>
<%end%>
	